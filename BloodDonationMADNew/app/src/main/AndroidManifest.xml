<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="my.edu.utar.blooddonationmadnew">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.BloodDonationMADNew">

        <activity
            android:name=".ui.AdminMainActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".ui.AdminAddBloodEventActivity"
            android:exported="false" />

        <activity
            android:name=".ui.UserMainActivity"
            android:exported="false" />

        <activity
            android:name=".sample.TestActivity"
            android:exported="false" />

        <activity
            android:name=".sample.TestAddActivity"
            android:exported="false" />

        <activity
            android:name=".sample.TestEditActivity"
            android:exported="false" />
    </application>

</manifest>